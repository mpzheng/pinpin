page{
  width: 100%;
  height: 100%;
}

.background {
  width: 100%;
  height: 100%;
  position:fixed; 
  z-index: -1;
}

.titel{
  height: 175rpx;
  width: 550rpx;
  background-color: rgb(255, 255, 255,0.7);
  border-bottom-right-radius: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.titel image{
  height: 100rpx;
  width: 100rpx;
  border-radius: 50rpx;
}
.titel text{
  font-size: 40rpx;
  margin-left: 5%;
}

.content{
  padding-top: 30rpx;
  margin-top: 20rpx;
  width: 550rpx;
  height: 1200rpx;
  border-top-right-radius: 20rpx;
  background-color: rgb(255, 255, 255,0.7);
}

.content-item{
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 3rpx solid white;
  padding-left: 75rpx;
  padding-right: 50rpx;
}